#include "main.h"
#include <unistd.h>
#include <stdio.h>
/**
 * main - prints alphabet
 *
 * print_alphabet - prints the alphabet with
 * a new line
 *
 * Return: 0
 */

int main(void)
{
	void print_alphabet(void)
	{
		char alphabet = 'a';

		while (alphabet <= 'z')
		{
			putchar(alphabet);
			alphabet++;
		}
		putchar('\n');
	}
	return (0);
}
