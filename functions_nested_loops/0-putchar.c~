#include <stdio.h>
#include "main.h"
/**
 * main - prints _putchar followed by
 * a new line
 * Return: 0
 */
int main(void)
{
	char _;
	char p;
	char u;
	char t;
	char c;
	char h;
	char a;
	char r;
		
	_putchar(_);
	_putchar(p);
	_putchar(u);
	_putchar(t);
	_putchar(c);
	_putchar(h);
	_putchar(a);
	_putchar(r);
	_putchar('\n');
	return (0);
}
