#include <stdlib.h>
#include <stdio.h>
#include <time.h>
/**
 * betty style doc for function main goes there 
 */
int main(void)
{
	int n, last_digit;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	scanf("%d",&n);
	last_digit = n % 10;
	printf("Last digit of %d is %d", n, last_digit);
	return (0);
}
